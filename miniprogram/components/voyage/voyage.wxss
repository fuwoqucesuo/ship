.voyage {
  width: 100%;
  border-radius: 10rpx;
  /* min-height: 200rpx; */
  margin-bottom: 16rpx;
}
.voyage .gap {
  width: 100%;
  height: 6rpx;
  background: #eeeeee;
}
.voyage .voyage-date {
  display: flex;
  justify-content: space-between;
  height: 70rpx;
  align-items: center;
}
.voyage .voyage-item {
  display: flex;
  width: 100%;
  height: 50rpx;
  align-items: center;
  color: #666666;
  margin-top: 10rpx;
}
.voyage .voyage-item .detail {
  flex: 1;
  display: flex;
  border-bottom: 1px solid #EEEEEE;
  align-items: center;
}
.voyage .voyage-item .detail .shipname {
  flex: 1;
}
.voyage .voyage-item .detail .mileage {
  width: 35%;
  text-align: center;
}
.voyage .voyage-item .detail .record {
  width: 10%;
  text-align: center;
}
.voyage .voyage-item .detail .data {
  font-size: 36rpx;
}
