.wrapper-detail {
  width: 100%;
  height: 100%;
  background: #EEEEEE;
}
.wrapper-detail .head {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 200rpx;
  background: #FFFFFF;
}
.wrapper-detail .head .head-img {
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  border: 1px solid #EEEEEE;
  overflow: hidden;
}
.wrapper-detail .reset-password {
  background: #FFFFFF;
  margin-top: 10rpx;
  padding: 0 16rpx;
  height: 80rpx;
  display: flex;
  align-items: center;
}
.wrapper-detail .reset-password .newpassword {
  flex: 1;
  height: 70rpx;
  margin: 0 10rpx;
}
.wrapper-detail .reset-password .resetBtn {
  width: 150rpx;
  color: #FFFFFF;
  background-color: #1296db;
}
.wrapper-detail .loginOut {
  margin-top: 10rpx;
  color: #333333;
  border-radius: 5rpx;
  width: 100%;
  background: #FFFFFF;
  font-weight: 500;
}
.wrapper-detail .savebtn {
  position: absolute;
  bottom: 35rpx;
  left: 50%;
  transform: translate(-50%);
  width: 80%;
  color: #FFFFFF;
  background-color: #1296db;
}
