.wrapper-list {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
}
.wrapper-list .list {
  padding: 16rpx;
  flex: 1;
  overflow: scroll;
}
.wrapper-list .nodata {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 36rpx;
  color: #666666;
}
.wrapper-list .add {
  width: 100%;
  height: 130rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #FFFFFF;
}
.wrapper-list .add .addbtn {
  width: 80%;
  color: #FFFFFF;
  background-color: #1296db;
}
