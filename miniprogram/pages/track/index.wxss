.wrapper-track {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
}
.wrapper-track .tabs {
  width: 100%;
  display: flex;
  height: 100rpx;
  align-items: center;
  background-color: #7dc5eb;
  justify-content: center;
  padding: 10rpx 0;
}
.wrapper-track .tabs .wrapper-tab {
  height: 100%;
  width: 96%;
  border: 1px solid #FFFFFF;
  border-radius: 10rpx;
  display: flex;
  align-content: center;
}
.wrapper-track .tabs .wrapper-tab .tab {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100%;
  font-size: 28rpx;
  font-weight: 600;
}
.wrapper-track .tabs .wrapper-tab .tab.selected {
  background-color: #FFFFFF;
  color: #7dc5eb;
}
.wrapper-track .swiper-content {
  flex: 1;
  overflow: scroll;
}
.wrapper-track .swiper-content .swiper-item {
  width: 100%;
}
.wrapper-track .swiper-content .swiper-item .swiper-item-info {
  width: 100%;
  padding: 0 20rpx;
}
.wrapper-track .swiper-content .swiper-item .swiper-item-info .todayNoData {
  font-size: 40rpx;
  color: #666666;
  text-align: center;
  margin: 30rpx 0;
}
.wrapper-track .swiper-content .swiper-item .swiper-item-info .times {
  height: 100rpx;
  display: flex;
  align-items: center;
  margin-bottom: 60rpx;
}
.wrapper-track .swiper-content .swiper-item .swiper-item-info .times .bridge {
  width: 30rpx;
  text-align: center;
}
.wrapper-track .swiper-content .swiper-item .swiper-item-info .times .item {
  height: 70%;
  flex: 1;
  border: 1px solid #EEEEEE;
  border-radius: 8rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.wrapper-track .swiper-content .swiper-item .swiper-item-info .showmore {
  text-align: center;
  height: 50rpx;
}
